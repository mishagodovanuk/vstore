{
    "name": "vstore/test",
    "type": "library",
    "description": "Simple mvc system",
    "license": "MIT",
    "keywords": [
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "symfony/http-foundation": "^6.4",
        "php-di/php-di": "^7.0",
        "ext-ctype": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "doctrine/dbal": "~2.0",
        "phpunit/phpunit": "^8.5 || ^9.4"
    },
    "autoload": {
        "psr-4": {
            "Vstore\\Router\\": "src",
            "Controller\\": "Controller",
            "Model\\": "Model",
            "Middlewares\\": "Middlewares"
        },
        "psr-0": {
            "Vstore\\Router\\": "src",
            "Model\\": "Model,"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vstore\\Test\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
